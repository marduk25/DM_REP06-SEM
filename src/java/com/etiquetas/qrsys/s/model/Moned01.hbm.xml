<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.etiquetas.qrsys.s.model.Moned01" table="MONED01" schema="dbo" catalog="SAE70Empre01" optimistic-lock="version">
        <id name="numMoned" type="int">
            <column name="NUM_MONED" />
            <generator class="assigned" />
        </id>
        <property name="descr" type="string">
            <column name="DESCR" length="20" />
        </property>
        <property name="simbolo" type="string">
            <column name="SIMBOLO" length="4" />
        </property>
        <property name="tcambio" type="java.lang.Double">
            <column name="TCAMBIO" precision="53" scale="0" />
        </property>
        <property name="fultcamb" type="timestamp">
            <column name="FULTCAMB" length="23" />
        </property>
        <property name="status" type="string">
            <column name="STATUS" length="1" />
        </property>
        <property name="uuid" type="string">
            <column name="UUID" length="50" />
        </property>
        <property name="versionSinc" type="timestamp">
            <column name="VERSION_SINC" length="23" />
        </property>
        <property name="cveMoned" type="string">
            <column name="CVE_MONED" length="4" />
        </property>
    </class>
</hibernate-mapping>
