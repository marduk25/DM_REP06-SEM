<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.etiquetas.qrsys.s.model.Mult01" table="MULT01" schema="dbo" catalog="SAE70Empre01" optimistic-lock="version">    
        <id name="cveArt" type="string">
            <column name="CVE_ART" length="16" />
            <generator class="assigned"/>
        </id>
        <property name="cveAlm" type="int">
            <column name="CVE_ALM" />
        </property>
        <property name="status" type="string">
            <column name="STATUS" length="1" />
        </property>
        <property name="ctrlAlm" type="string">
            <column name="CTRL_ALM" length="10" />
        </property>
        <property name="exist" type="java.lang.Double">
            <column name="EXIST" precision="53" scale="0" />
        </property>
        <property name="stockMin" type="java.lang.Double">
            <column name="STOCK_MIN" precision="53" scale="0" />
        </property>
        <property name="stockMax" type="java.lang.Double">
            <column name="STOCK_MAX" precision="53" scale="0" />
        </property>
        <property name="compXRec" type="java.lang.Double">
            <column name="COMP_X_REC" precision="53" scale="0" />
        </property>
        <property name="uuid" type="string">
            <column name="UUID" length="50" />
        </property>
        <property name="versionSinc" type="timestamp">
            <column name="VERSION_SINC" length="23" />
        </property>
    </class>
</hibernate-mapping>
