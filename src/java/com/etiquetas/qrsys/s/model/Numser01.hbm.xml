<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.etiquetas.qrsys.s.model.Numser01" table="NUMSER01" schema="dbo" catalog="SAE70Empre01" optimistic-lock="version">
        <id name="cveArt" type="string">
            <column name="CVE_ART" length="16" />
            <generator class="assigned"/>
        </id>
        <property name="numSer" type="string">
            <column name="NUM_SER" length="25" />
        </property>
        <property name="almacen" type="int">
            <column name="ALMACEN" />
        </property>
        <property name="status" type="string">
            <column name="STATUS" length="1" />
        </property>
        <property name="costo" type="java.lang.Double">
            <column name="COSTO" precision="53" scale="0" />
        </property>
        <property name="doctoEnt" type="string">
            <column name="DOCTO_ENT" length="20" />
        </property>
        <property name="fechaEnt" type="timestamp">
            <column name="FECHA_ENT" length="23" />
        </property>
    </class>
</hibernate-mapping>
